@model ProjectModel
@section header{
    <h1>
        Project: @Model.ProjectName
        <small>Project code: @Model.ProjectCode</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> Dashboard</a></li>
        <li><a asp-action="Index" asp-controller="Project">Project</a></li>
        <li class="active">@Model.ProjectCode</li>
    </ol>
}

    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title"><i class="fa fa-bandcamp"></i> Project information</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="" data-original-title="Collapse">
                    <i class="fa fa-minus"></i>
                </button>
            </div>
        </div>
        <div class="box-body">
            <div class="row">
                <div class="col-xs-6">
                    ONE
                </div>
                <div class="col-xs-6">
                    TWO
                </div>
            </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
            Footer
        </div>
        <!-- /.box-footer-->
    </div>